LABEL 1
        MENU LABEL ^1) Rescue CentOS 7 x64 over the network with Pixies
        MENU DEFAULT
        KERNEL centos7/vmlinuz
        # CHANGE THE FTP ADDRESS ON THE NEXT LINE (e.g. ftp://ftp:ftp@<mgmt_node_ip>/pub/anaconda-ks.cfg)
        APPEND initrd=centos7/initrd.img 
	
LABEL local
        MENU LABEL ^2) local boot
        COM32 chain.c32
        APPEND hd0
